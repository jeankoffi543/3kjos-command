{
  "version" : "1.0.2",
  "name": "kjos/command",
  "description": "Command tool for Laravel and other",
  "authors": [
    {
      "name": "Koffi Konan Kan Jean Sylvain",
      "email": "jeankoffi543@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/jeankoffi543/3kjos-command.git",
      "options": {
        "symlink": true
      }
    }
  ],
  "require": {
    "php": "^7.0|^8.0",
    "illuminate/support": "^8.0",
    "illuminate/database": "^8.0|^11.0",
    "illuminate/console": "*",
    "illuminate/routing": "*",
    "laravel/pint": "^1.20"
},
  "autoload": {
    "psr-4": {
      "Kjos\\Command\\": "src/"
    },
    "files":[
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
        "Kjos\\Command\\Tests\\": "tests/"
    }
},
  "extra": {
    "laravel": {
      "providers": [
        "Kjos\\Command\\CommandServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpro/grumphp": true,
      "phpstan/extension-installer": true
    }
  },
  "scripts": {
    "test" : "vendor/bin/pest"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": "^6.0", 
    "mockery/mockery": "^1.4",
    "phpstan/phpstan": "^0.12",
    "nunomaduro/collision": "^5.0",
    "fakerphp/faker": "^1.23",
    "pestphp/pest": "^1.23",
    "pestphp/pest-plugin-laravel": "^1.4",
    "phpro/grumphp": "^2.5"
  }
}
